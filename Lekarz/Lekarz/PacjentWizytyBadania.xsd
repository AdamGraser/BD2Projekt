                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Lekarz.nazwisko + ' ' + Lekarz.imie AS Lek_Lekarz, Sl_specjalizacji.nazwa AS Sl_spec_nazwa, Wizyta.data_rej AS Wiz_data_rej, Wizyta.opis AS Wiz_opis, 
                         Wizyta.diagnoza AS Wiz_diagnoza, Pacjent.nazwisko + ' ' + Pacjent.imie AS Pac_Pacjent, Badanie.data_wyk_bad AS Bad_data_wyk_bad, 
                         Badanie.opis AS Bad_opis, Badanie.wynik AS Bad_wynik, Badanie.uwagi AS Bad_uwagi, Sl_badan.nazwa AS Sl_bad_nazwa, Sl_badan.opis AS Sl_bad_opis, 
                         CASE Wizyta.stan WHEN 0 THEN 'zarejestrowana' WHEN 1 THEN 'realizowana' WHEN 2 THEN 'anulowana' WHEN 3 THEN 'zakończona' END AS Wiz_stan, 
                         CASE Badanie.stan WHEN 0 THEN 'zlecone' WHEN 1 THEN 'anulowane LAB' WHEN 2 THEN 'wykonane' WHEN 3 THEN 'anulowane KLAB' WHEN 4 THEN 'zatwierdzone'
                          END AS Bad_stan, Pacjent.id_pac, Wizyta.id_wiz, Badanie.id_bad, 
                         CASE Sl_badan.lab WHEN 0 THEN 'Badania fizykalne' WHEN 1 THEN 'Badania laboratoryjne' ELSE NULL END AS Sl_bad_lab
FROM            Sl_specjalizacji INNER JOIN
                         Lekarz ON Sl_specjalizacji.kod_spec = Lekarz.kod_spec INNER JOIN
                         Wizyta ON Lekarz.id_lek = Wizyta.id_lek INNER JOIN
                         Pacjent ON Pacjent.id_pac = Wizyta.id_pac LEFT OUTER JOIN
                         Badanie ON Badanie.id_wiz = Wizyta.id_wiz LEFT OUTER JOIN
                         Sl_badan ON Badanie.kod = Sl_badan.kod
WHERE        (Pacjent.id_pac = @IdPac)
ORDER BY Pacjent.id_pac, Wiz_data_rej</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="IdPac" ColumnName="id_pac" DataSourceName="Przychodnia.dbo.Pacjent" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@IdPac" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="id_pac" SourceColumnNullMapping="false" SourceVersion="Current" />
